        stage('Analisis Sonarqube') {
            steps {
                script {
                    dir('my-app') {
                        deleteDir() // Limpiar el directorio de trabajo
                        withSonarQubeEnv(installationName: 'sq1') {
                            sh "mvn sonar:sonar"
                        }
                    }
                }
            }
        }
